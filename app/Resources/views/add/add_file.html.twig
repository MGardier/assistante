{% extends 'base.html.twig' %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('css/formulaires/creerFile.css') }}">
    {{ parent() }}
{% endblock %}
{% block body %}
    <h1>Ajouter un nouveau fichier</h1>
    <div class="container">
        <div class="formulaire">
            {{ form_start(form) }}
            <div class="form-group centrage">{{ form_row(form.fichier) }}</div>
            <div class="form-group float-right">{{ form_row(form.Enregistrer) }}</div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock body %}