{% extends 'base.html.twig' %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('css/formulaires/creerChambre.css') }}">
    {{ parent() }}
{% endblock %}
{% block body %}
    <div class="container">
        <div class="formulaire">
            {{ form_start(form) }}
            <div class="form-group">{{ form_row(form.service) }}</div>
            <div class="form-group">{{ form_row(form.numero) }}</div>
            <div class="form-group">{{ form_row(form.dateEntrer) }}</div>
            <div class="form-group">{{ form_row(form.dateSortie) }}</div>
            <div class="form-group">{{ form_row(form.Enregistrer) }}</div>
            {{ form_end(form) }}
            <a href="{{ path('ref',{'id':id}) }}">
                <button class="boutton-suivant">Suivant</button>
            </a>
        </div>
    </div>
{% endblock body %}

